    <td>
        @Html.DisplayFor(modelItem => item.DegistirmeTarihi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DegistirenKullanici)
    </td>
    <td>
        @BootstrapHelper.Linkbuton("/Kategori/Edit/" + item.Id, "warning", "btn-sm", "edit", "")
        @BootstrapHelper.Linkbuton("/Kategori/Details/" + item.Id, "info", "btn-sm", "search", "")
        @BootstrapHelper.Linkbuton("/Kategori/Delete/" + item.Id, "danger", "btn-sm", "trash", "")
    </td>
</tr>
}

</table>
